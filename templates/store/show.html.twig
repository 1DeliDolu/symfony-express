{% extends 'base.html.twig' %}

{% block title %}Store{% endblock %}

{% block body %}
    <h1>Store</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>StorId</th>
                <td>{{ store.storId }}</td>
            </tr>
            <tr>
                <th>StorName</th>
                <td>{{ store.storName }}</td>
            </tr>
            <tr>
                <th>StorAddress</th>
                <td>{{ store.storAddress }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ store.city }}</td>
            </tr>
            <tr>
                <th>State</th>
                <td>{{ store.state }}</td>
            </tr>
            <tr>
                <th>Zip</th>
                <td>{{ store.zip }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_store_index') }}">back to list</a>

    <a href="{{ path('app_store_edit', {'storId': store.storId}) }}">edit</a>

    {{ include('store/_delete_form.html.twig') }}
{% endblock %}
