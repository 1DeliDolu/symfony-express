{% extends 'base.html.twig' %}

{% block title %}TitleAuthor index{% endblock %}

{% block body %}
    <h1>TitleAuthor index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>AuOrd</th>
                <th>RoyaltyPer</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for title_author in title_authors %}
            <tr>
                <td>{{ title_author.auOrd }}</td>
                <td>{{ title_author.royaltyPer }}</td>
                <td>
                    <a href="{{ path('app_title_author_show', {'author': title_author.author}) }}">show</a>
                    <a href="{{ path('app_title_author_edit', {'author': title_author.author}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_title_author_new') }}">Create new</a>
{% endblock %}
